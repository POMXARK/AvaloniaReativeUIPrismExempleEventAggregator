             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaReativeUIPrismExempleEventAggregator.Views.UseCommandWindow">

  <StackPanel>
    <Label Content="Welcome to UseCommandWindow!"/>
    <Button Content="Update" Command="{Binding DoTheThing}"/>
    <TextBox Text="{Binding Rx3}"/>
    <Label Content="{Binding Rx4}"/>
  </StackPanel>
  
</UserControl>
